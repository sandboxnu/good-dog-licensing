{
  "extends": "@good-dog/typescript/base.json",
  "compilerOptions": {
    "lib": ["es2022", "dom", "dom.iterable"],
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "~/*": ["app/*"]
    },
    "tsBuildInfoFile": "node_modules/.cache/tsbuildinfo.json",
    "module": "esnext",
    "types": ["react/canary"],
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [".", ".next/types/**/*.ts"],
  "exclude": ["node_modules", ".next"]
}
